<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="620.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.regpagehw.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="480.0" prefWidth="620.0">
      <children>
      
          <Label fx:id="welcomeText" layoutX="25.0" layoutY="100.0" text="Please enter your first name">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="firstText" layoutX="25.0" layoutY="120.0" onKeyTyped="#firstTyped" prefHeight="25.0" prefWidth="240.0" />
         <Label fx:id="welcomeText1" layoutX="25.0" layoutY="160.0" text="Please enter your last name">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="lastText" layoutX="25.0" layoutY="180.0" onKeyTyped="#lastTyped" prefHeight="25.0" prefWidth="240.0" />
         <Label fx:id="welcomeText2" layoutX="25.0" layoutY="220.0" text="Please enter your email">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="emailText" layoutX="25.0" layoutY="240.0" onKeyTyped="#emailTyped" prefHeight="25.0" prefWidth="240.0" />
         <Label fx:id="welcomeText3" layoutX="25.0" layoutY="280.0" text="Please enter your date of birth">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="dobText" layoutX="25.0" layoutY="300.0" onKeyTyped="#dobTyped" prefHeight="25.0" prefWidth="240.0" />
         <Button fx:id="addUser" disable="true" layoutX="25.0" layoutY="400.0" onAction="#addUserButtonClicked" text="Add User" />
         <Label fx:id="welcomeText4" layoutX="25.0" layoutY="340.0" text="Please enter your zip code">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="zipText" layoutX="25.0" layoutY="360.0" onKeyTyped="#zipTyped" prefHeight="25.0" prefWidth="240.0" />
         <Label layoutX="24.0" layoutY="38.0" text="Welcome to the registration page!">
            <font>
               <Font size="25.0" />
            </font>
         </Label>
      </children>
   </AnchorPane>
</VBox>
